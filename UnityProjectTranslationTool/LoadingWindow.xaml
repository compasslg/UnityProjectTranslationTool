<Window x:Class="UnityProjectTranslationTool.LoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UnityProjectTranslationTool"
        mc:Ignorable="d"
        Title="LoadingWindow" Height="300" Width="400">
    <StackPanel>
        <TextBox x:Name="ProgressText" Height="250"/>
        <Button x:Name="Complete" Content="Complete" Width="60" Height="20" Click="Complete_Click"/>
    </StackPanel>
</Window>
